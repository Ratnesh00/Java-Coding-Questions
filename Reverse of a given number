import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    
	    
		System.out.print("Enter a number : ");
		int n = sc.nextInt();
		int reverse = 0;
		//int remainDer;
		
		while(n!=0){
		    //remainDer = n%10;
		    reverse = (reverse*10)+(n%10);
		    n = n/10;
		}
		System.out.println("Reverse of given number is : " + reverse);
	}
}
